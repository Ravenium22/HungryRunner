{
    "version": 2,
    "builds": [
      { "src": "public/**/*.html", "use": "@vercel/static" },
      { "src": "public/styles/**/*.css", "use": "@vercel/static" },
      { "src": "public/sprites/**/*", "use": "@vercel/static" },
      { "src": "public/audio/**/*", "use": "@vercel/static" },
      { "src": "public/src/**/*.js", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/$1" }
    ]
  }
  